{
    "name": "vite-plugin-file-router",
    "version": "1.0.11",
    "description": "file-router plugin for vite",
    "keywords": [
      "vite-plugin",
      "react",
      "router",
      "file-system-routing"
    ],
    "author": "Gangho Lee",
    "license": "MIT",
    "repository": {
      "type": "git",
      "url": "https://github.com/chickengir1/vite-plugin-file-router"
    },
    "type": "module",
  
    "main": "dist/vite-plugin-file-router.mjs",
    "types": "./dist/index.d.ts",
  
    "exports": {
      ".": {
        "import": "./dist/vite-plugin-file-router.mjs",
        "require": "./dist/vite-plugin-file-router.mjs",
        "types": "./dist/index.d.ts"
      }
    },
    "files": [
      "dist"
    ],
  
    "scripts": {
      "dev": "vite",
      "prepare": "npm run build",
      "build": "tsc --declaration --emitDeclarationOnly && tsc && node scripts/rename-js-to-mjs.js"
    },
  
    "peerDependencies": {
      "react": ">=18.0.0",
      "react-router-dom": ">=6.0.0",
      "vite": ">=4.0.0"
    },
    "devDependencies": {
      "@types/react": "^19.0.8",
      "typescript": "^5.0.0",
      "vitest": "^0.34.0"
    }
  }
  